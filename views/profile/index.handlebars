  <header>
  <div class="container profile-name-cover">
    {{#if randomPerson.coverPicture}}
      <img src = "images/background.jpg" class="cover-image"/>
    {{else}}
       <img src = "images/background.jpg" class="cover-image"/>
    {{/if}}
    <div class="row" align="center">
      <div class="col-md-4 col-md-push-4">
          {{#if randomPerson.profilePicture}}
            <img src= "images/profile-default.jpg" class="profile-picture img-circle"/>
          {{else}}              
            {{!-- <form action="/images" class="dropzone profile-picture img-circle dropzoneForm">
            </form> --}}
            <img src= "images/profile-default.jpg" class="profile-picture img-circle" id="myId"/>
            <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
            <script type="text/javascript">
              $(document).ready(function(){
                $("img#myId").dropzone({ url: "/images/profile-default.jpg" });
              });
            </script>
          {{/if}}
      </div>
    </div>
    <div class="row" align="center">
      <div class="col-md-6 col-md-push-3">
        <h1>{{ randomPerson.firstName }} {{ randomPerson.lastName }}'s Account</h1>
      </div>
    </div>
  </div>
</header>