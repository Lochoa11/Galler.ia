<header class="cover-image">
  <div class="container profile-name-cover">
  	<div class="row profile-name">
	    <div class="row" align="center">
	      <div class="col-md-4 col-md-push-4">
	        <img src="images/profile-default.jpg" class="img-responsive img-circle center-block profile-picture">
	      </div>
	    </div>
	    <div class="row" align="center">
	      <div class="col-md-6 col-md-push-3">
	        <h1>{{ user.firstName }} {{ user.lastName }}'s Account</h1>
	      </div>
	    </div>
  	</div>
  </div>
</header>

<div class="container profile-content push-down">
	<div class="row">
		<a class="btn btn-info" href="http://localhost:8000/profile/upload/"> Upload Photo</a>
	</div>
</div>

<div class"image_grid">
		{{#each photos}}
			<div class="col-md-4">
				<a href="/photos/{{this.id}}"><img class="img-responsive thumbnail" src={{this.image_url}} /> </a>
			</div>
		{{/each}}
</div>
